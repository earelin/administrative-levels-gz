{
  "name": "adminstrative-levels-gz",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "acceptance-test": "jest acceptance-test acceptance-test",
    "build": "docker build --tag administrative-levels-gz:latest .",
    "dev": "nodemon src/index.js",
    "lint": "eslint src/**/*.js __test__/**/*.js",
    "nomenclator-extract": "node src/nomenclator",
    "restart": "docker kill admin-levels-gz-dev && docker build --tag administrative-levels-gz:latest . && docker run -d -p 9000:9000 --rm --name admin-levels-gz-dev administrative-levels-gz:latest",
    "shapefiles-extract": "node src/shapefiles",
    "start": "docker build --tag administrative-levels-gz:latest . && docker run -d -p 9000:9000 --rm --name admin-levels-gz-dev administrative-levels-gz:latest",
    "stop": "docker kill admin-levels-gz-dev",
    "test": "jest __test__"
  },
  "devDependencies": {
    "eslint": "^8.21.0",
    "eslint-plugin-jest": "^26.8.2",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "pactum": "^3.1.13"
  },
  "bin": {
    "nomenclator-extract": "src/nomenclator/index.js",
    "shapefiles-extract": "src/shapefiles/index.js"
  },
  "dependencies": {
    "csv-parser": "^3.0.0",
    "express": "^4.18.1",
    "lodash": "^4.17.21",
    "shapefile": "^0.6.6",
    "yargs": "^17.5.1"
  },
  "jest": {
    "globals": {
      "TEST_BASE_URL": "http://127.0.0.1:9000"
    },
    "testMatch": [
      "**/?(*.)+(it|spec|test).[jt]s?(x)"
    ]
  }
}
